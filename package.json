{
  "name": "explicolearning",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development vite --host 0.0.0.0 --port 3000",
    "dev:debug": "NODE_ENV=development vite --host 0.0.0.0 --port 3000 --debug",
    "build": "NODE_ENV=production vite build",
    "preview": "NODE_ENV=production vite preview --host 0.0.0.0 --port 4173",
    "preview:local": "npm run build && npm run preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "node --max-old-space-size=8192 node_modules/.bin/vitest run",
    "typecheck": "node_modules/.bin/tsc --noEmit --skipLibCheck",
    "lint": "npm run typecheck && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:circular": "eslint 'src/**/*.{js,jsx,ts,tsx}' --rule 'import/no-cycle: error' --no-eslintrc --env es6 --parser-options '{\"ecmaVersion\": 2022, \"sourceType\": \"module\"}' --ext .ts,.tsx,.js,.jsx 2>&1 | grep -E '(import/no-cycle|error|warning.*no-cycle)' || echo 'No circular dependencies found'",
    "lint:eslint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "deps:check": "madge --circular src/",
    "deps:graph": "madge --image dependency-graph.svg src/",
    "deps:summary": "madge --summary src/",
    "test:errors": "npm run test:run -- ReactErrorDetection",
    "lint:comprehensive": "npm run typecheck && npm run lint:eslint && npm run deps:check",
    "verify:production": "npm run lint:comprehensive && npm run test:errors && npm run build",
    "analyze:bundle-size": "vite build --mode analyze && npx vite-bundle-analyzer dist",
    "analyze:component-deps": "madge --image component-deps.svg src/client/components/",
    "analyze:circular-deps": "madge --circular --warning src/",
    "analyze:unused-exports": "npx unimported",
    "test:performance-baseline": "vitest run --reporter=verbose src/tests/performance/",
    "test:memory-usage": "vitest run src/tests/performance/MemoryUsage.test.tsx",
    "test:render-optimization": "vitest run src/tests/performance/RenderOptimization.test.tsx",
    "test:mobile-performance": "vitest run src/tests/mobile-ux/PerformanceOptimization.test.tsx",
    "firebase:build": "NODE_ENV=production npm run build",
    "firebase:deploy": "firebase deploy --only hosting",
    "firebase:serve": "firebase serve --only hosting --host 0.0.0.0",
    "firebase:emulator": "firebase emulators:start --only hosting,firestore",
    "deploy": "npm run firebase:build && npm run firebase:deploy",
    "deploy:all": "npm run firebase:build && firebase deploy",
    "backup": "npx ts-node scripts/backup-data.ts",
    "codespaces:setup": "npm install && npm run dev",
    "codespaces:preview": "npm run build && npm run firebase:serve",
    "puppeteer:test": "node scripts/test-puppeteer.js",
    "puppeteer:analyze": "node scripts/analyze-console.js",
    "mcp:server": "node scripts/mcp-puppeteer-server.js",
    "mcp:inspect": "npx @modelcontextprotocol/inspector",
    "mcp:test": "npx @hisma/server-puppeteer",
    "mcp:workflow": "node scripts/mcp-dev-workflow.js",
    "mcp:validate": "node scripts/validate-mcp-config.js",
    "mcp:demo": "node scripts/mcp-dev-workflow.js demo",
    "test:auth": "node scripts/puppeteer-auth-helper.js",
    "test:responsive-patterns": "vitest run src/tests/responsive-patterns.test.tsx",
    "test:performance": "vitest run src/tests/mobile-ux/PerformanceOptimization.test.tsx",
    "test:memory-leaks": "node --expose-gc scripts/memory-leak-test.js",
    "test:touch-performance": "vitest run src/tests/mobile-ux/PerformanceOptimization.test.tsx --reporter=verbose",
    "build:analyze": "npm run build && npx rollup-plugin-visualizer dist/stats.html",
    "test:visual-regression": "percy-playwright test",
    "test:responsive-layouts": "vitest run src/tests/responsive-layouts.test.tsx",
    "test:mobile-interactions": "vitest run src/tests/mobile-interactions.test.tsx",
    "test:cross-browser": "playwright test --browser all",
    "test:accessibility": "playwright test --project=accessibility",
    "test:screen-reader": "echo \"Screen reader tests need manual execution\"",
    "test:keyboard-navigation": "vitest run src/tests/keyboard-navigation.test.tsx",
    "test:aria-compliance": "vitest run src/tests/aria-compliance.test.tsx"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@types/classnames": "^2.3.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "browser-image-compression": "^2.0.2",
    "classnames": "^2.5.1",
    "dompurify": "^3.2.6",
    "firebase": "^10.14.1",
    "firebase-admin": "^13.4.0",
    "framer-motion": "^12.23.9",
    "lodash.debounce": "^4.0.8",
    "puppeteer": "^24.14.0",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-pull-to-refresh": "^2.0.1",
    "react-router-dom": "^7.6.3",
    "react-swipeable": "^7.0.2",
    "react-window": "^1.8.11",
    "react-zoom-pan-pinch": "^3.7.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.9.1",
    "@babel/generator": "^7.28.0",
    "@babel/parser": "^7.28.0",
    "@babel/traverse": "^7.28.0",
    "@eslint/eslintrc": "^3.3.1",
    "@hisma/server-puppeteer": "^0.6.5",
    "@modelcontextprotocol/sdk": "^1.16.0",
    "@percy/playwright": "^1.0.9",
    "@playwright/test": "^1.55.0",
    "@storybook/test-runner": "^0.19.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^22.14.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "chromatic": "^11.5.4",
    "eslint": "^9.32.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "glob": "^11.0.3",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "madge": "^8.0.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.17",
    "terser": "^5.42.0",
    "typescript": "~5.7.2",
    "undici-types": "^6.20.0",
    "vite": "^5.4.11",
    "vite-plugin-circular-dependency": "^0.5.0",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "rimraf": "^6.0.0"
  }
}
