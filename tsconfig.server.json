{
  "compilerOptions": {
    "target": "ES2015",
    "module": "None",
    "lib": ["ES2015"],
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": false,
    "outDir": "./dist",
    "rootDir": "./src",
    "declaration": false,
    "removeComments": true,
    "types": ["google-apps-script"]
  },
  "include": [
    "src/server/**/*",
    "src/shared/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}