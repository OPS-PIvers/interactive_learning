# Firebase Configuration - Replace with your production Firebase project details
VITE_FIREBASE_API_KEY=your_production_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_production_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_production_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_production_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_production_sender_id
VITE_FIREBASE_APP_ID=your_production_app_id

# Application Configuration
VITE_APP_TITLE="Hotspot Walkthroughs"
VITE_APP_DESCRIPTION="Create interactive onboarding experiences"
VITE_APP_URL=https://your-production-app-url.com

# Development Features - Should be disabled in production
VITE_DEV_AUTH_BYPASS=false
VITE_DEV_USER_EMAIL=
VITE_DEV_USER_NAME=

# Analytics (Optional) - Enable and configure for production
VITE_GOOGLE_ANALYTICS_ID=
VITE_HOTJAR_ID=

# API Configuration
VITE_API_BASE_URL=https://your-api.com
VITE_CDN_BASE_URL=https://your-cdn.com

# Feature Flags - Enable features for production as needed
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
